@model List<Category>

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2 class="">
                Category List
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="" asp-action="" class="btn btn-dark">
                <i class="bi bi-plus-square m-1"></i>Create New Category
            </a>
        </div>
    </div>
<table class="table table-bordered table-striped table-dark">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Category Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }

    </tbody>

</table>
</div>